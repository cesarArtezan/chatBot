{
  "name": "typescript-express-server",
  "version": "1.0.0",
  "main": "./build/index.js",
  "license": "MIT",
  "scripts": {
    "server": "node ./build/app.js",
    "start": "tsc && nodemon ./build/app.js",
    "ts:watch": "tsc -w",
    "build-doc": "apidoc -i src/ --silent"
  },
  "nodemonConfig": {
    "ignore": [
      "doc/*"
    ]
  },
  "apidoc": {
    "name": "cesar-users-api",
    "version": "0.1.0",
    "description": "Documentation for DemoApp",
    "url": "https://cesarapp12.herokuapp.com/api/v1",
    "sampleUrl": "https://cesarapp12.herokuapp.com/api/v1"
  },
  "dependencies": {
    "actions-on-google": "^2.1.1",
    "apidoc": "^0.17.6",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "^4.0.0",
    "express": "4.16.2",
    "helmet": "3.9.0",
    "lodash": "4.17.4",
    "mongoose": "5.0.0-rc1",
    "morgan": "1.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.3",
    "@types/express": "4.11.0",
    "@types/mongoose": "5.0.14",
    "@types/node": "8.5.5",
    "nodemon": "1.14.7",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  }
}
